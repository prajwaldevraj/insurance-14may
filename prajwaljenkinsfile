pipeline {
  agent any

  tools {
    maven 'M2_HOME'
        }

   stages {
     stage('Checkout') {
       steps {
           echo 'Checkout he source code form Github'
             https://github.com/prajwaldevraj/insurance-14may.git
             }
        }
    stage('Package') {
       steps {
          echo 'Create a Package'
          sh 'mvn clean package'
             }
        }
    }
}
